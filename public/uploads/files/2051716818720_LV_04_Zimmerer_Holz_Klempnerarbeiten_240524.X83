    <Remark ID="ID_141">
     <Description ID="80CB895A-3D24-42EA-B21F-FFCE1181C87E">
      <CompleteText>
       <DetailTxt>
        <Text textWidth="14.998cm">
         <p style="font-family:Arial;font-size:10pt;text-align:left;margin-top:0pt;margin-bottom:0pt;">
          <span style="font-family:Arial;font-weight:bold;font-size:10pt;">Projektbeteiligte</span>
         </p>
         <p style="font-family:Arial;font-size:10pt;text-align:left;margin-top:0pt;margin-bottom:0pt;">
          <span style="font-family:Arial;font-size:10pt;"> </span>
         </p>
         <p style="font-family:Arial;font-size:10pt;text-align:left;margin-top:0pt;margin-bottom:0pt;tab-stops:9.70mm 25.90mm 39.90mm 55.20mm 59.70mm 70.30mm 159.80mm 179.90mm 200.00mm 219.80mm 239.90mm 259.80mm 279.90mm 292.00mm;">
          <span style="font-family:Arial;font-weight:bold;font-size:10pt;">1. Bauvorhaben: </span>
          <span style="font-family:Arial;font-size:10;mso-tab-count:1;"/>
          <span style="font-family:Arial;font-size:10pt;">Neubau eines Zweifamilienhauses</span>
         </p>
         <p style="font-family:Arial;font-size:10pt;text-align:left;margin-top:0pt;margin-bottom:0pt;tab-stops:9.70mm 25.90mm 39.90mm 55.20mm 59.70mm 70.30mm 159.80mm 179.90mm 200.00mm 219.80mm 239.90mm 259.80mm 279.90mm 292.00mm  9.70mm 25.90mm 39.90mm 55.20mm 59.70mm 70.30mm 159.80mm 179.90mm 200.00mm 219.80mm 239.90mm 259.80mm 279.90mm 292.00mm;">
          <span style="font-family:Arial;font-size:10pt;"/>
          <span style="font-family:Arial;font-size:10;mso-tab-count:1;">          </span>
          <span style="font-family:Arial;font-size:10pt;"/>
          <span style="font-family:Arial;font-size:10;mso-tab-count:1;">               </span>
          <span style="font-family:Arial;font-size:10pt;"/>
          <span style="font-family:Arial;font-size:10;mso-tab-count:1;">             </span>
