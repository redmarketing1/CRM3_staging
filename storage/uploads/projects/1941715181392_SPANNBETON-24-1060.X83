         <BoQCtgy ID="id878" RNoPart="8">
          <LblTx>
           <p>
            <span>Betonstahl und Einbauteile</span>
           </p>
          </LblTx>
          <BoQBody>
           <Remark ID="A4JTXZQS">
            <Description>
             <CompleteText>
              <DetailTxt>
               <Text>
                <p><span style="font-size:medium;">Einbauteile</span></p>
               </Text>
              </DetailTxt>
              <OutlineText>
               <OutlTxt>
                <TextOutlTxt>
                 <p>
                  <span>Einbauteile</span>
                 </p>
