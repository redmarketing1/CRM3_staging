                      <Item ID="ID_610ee4af-01d0-4bce-b1ce-8b7841c2832d" RNoPart="0040">
                        <UPBkdn>No</UPBkdn>
                        <Qty>1</Qty>
                        <QU>St</QU>
                        <Description>
                          <CompleteText>
                            <ComplTSA>No</ComplTSA>
                            <ComplTSB>No</ComplTSB>
