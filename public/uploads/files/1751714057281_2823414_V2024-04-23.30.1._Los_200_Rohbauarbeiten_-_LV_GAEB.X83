        <Itemlist>
         <Item ID="AAAAAAAAIAKNJAAA" RNoPart="0010">
          <Qty>1.000</Qty>
          <QU>pau</QU>
          <Description>
           <CompleteText>
            <DetailTxt>
             <Text>
              <p style="text-align:left;margin-top:0pt;margin-bottom:0pt;">
               <span style="font-family:Arial;font-size:10pt;Color:rgb(0,0,0);">Aufstellen, betriebsbereit anschließen und nach Freigabe abbauen von Mietcontainern,</span>
              </p>
